-- pat_cons2 --
// This tests when the pattern of a constraint is a ref and when the ref's value changes,
// the previously invalid value is valid.
x: {
	[y.z]: {f1: 1}
	a: {f2: 2}
	b: 1 // previously invalid value if string is matched.
}
y: {
	z:     string
	("z"): =~"a"
}
-- exp --
x: {
    a: {
        f2: 2
        f1: 1
  }
    b: {
        f3: 3
  }
}
y: {
    z: =~a
    (z): =~"a"
}
