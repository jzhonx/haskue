-- selector1 --
// this is the example from the cuelang spec
T: {
	x:     1
	y:     3
	"x-y": 4
}
a: T.x     // 1
b: T.y     // 3
c: T.z     // _|_ // field 'z' not found in T
d: T."x-y" // 4
e: {a: 1 | *2} | *{a: 3 | *4}
f: e.a // 4 (default value)
-- exp_selector1 --
T: {
    x: 1
    y: 3
    x-y: 4
}
a: 1
b: 3
c: T.z
d: 4
e: {
    a: 4
}
f: 4

-- selector2 --
a: {
	b: {
		c: 42
	}
}
x: a.b.c
-- exp_selector2 --
a: {
    b: {
        c: 42
    }
}
x: 42

