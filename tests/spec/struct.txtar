x0: {
    f: 3
}
x1: {
    f: 3
}
x2: {
    f: int
}
x3: {
    f!: <1
}
x4: {
    f: <=3
}
x5: {
    f: 3
}
x6: {
    f: 3
}
x7: {
    f: 3
}

-- struct4 --
// dynamic fields
a:   "foo"
b:   "bar"
(a): "baz"
(a + b): "qux"
(a)?: string
(b)!: string
-- exp_struct4 --
a: "foo"
b: "bar"
foo: "baz"
foobar: "qux"
bar!: string

-- struct5 --
// This tests dynamic field can change an existing field.
a: {
	c: "b"
}
b: {
	x: "x"
}
(a.c): {
	y: "y"
}
-- exp_struct5 --
a: {
    c: "b"
}
b: {
    x: "x"
    y: "y"
}

